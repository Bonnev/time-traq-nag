{
	"applicationId": "js.neutralino.sample",
	"version": "1.0.0",
	"defaultMode": "window",
	"documentRoot": "/dist/",
	"url": "/",
	"enableServer": true,
	"enableNativeAPI": true,
	"tokenSecurity": "none", 
	"logging": {
		"enabled": true,
		"writeToLogFile": true
	},
	"nativeAllowList": [
		"filesystem.*",
		"window.*",
		"app.exit"
	],
	"globalVariables": {
		"TEST1": "Hello"
	},
	"modes": {
		"window": {
			"title": "time-traq-nag",
			"width": 470,
			"height": 250,
			"__minWidth__": 400,
			"__minHeight__": 200,
			"fullScreen": false,
			"alwaysOnTop": false,
			"icon": "/public/neutralino.png",
			"enableInspector": false,
			"borderless": false,
			"maximize": false,
			"hidden": false,
			"resizable": true,
			"center": true,
			"exitProcessOnClose": false
		}
	},
	"cli": {
		"binaryName": "neutralinojs",
		"resourcesPath": "/dist/",
		"extensionsPath": "/extensions/",
		"binaryVersion": "5.1.0",
		"clientVersion": "5.1.0",
		"frontendLibrary": {
			"patchFile": "./index.html",
			"projectPath": "./",
			"devUrl": "http://localhost:5173",
			"devCommand": "yarn vite-dev",
			"buildCommand": "yarn vite-build"
		}
	}
}